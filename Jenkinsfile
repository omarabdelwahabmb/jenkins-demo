pipeline {
    agent any
            
    stages {
        stage('Hello') {
            steps {
                echo 'Hello, from git!'
            }
        }
        stage('Hook') {
            steps {
                echo 'Hook Worked, from git!';
            }
        }
        stage('Bye') {
            steps {
                echo 'Bye, from git!'
            }
        }
    }
}
